{
  "name": "jupytext-sync",
  "displayName": "Jupytext Paired Jupyter Notebooks",
  "description": "Auto sync paired Jupyter notebooks via Jupytext",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.95.0"
  },
  "license": "BSD-3-Clause",
  "categories": [
    "Notebooks",
    "Data Science",
    "Programming Languages",
    "Visualization",
    "Machine Learning"
  ],
  "keywords": [
    "python",
    "jupyter",
    "jupytext",
    "nbconvert",
    "notebook",
    "notebooks",
    "synced",
    "paired"
  ],
  "publisher": "caenrigen",
  "homepage": "https://github.com/caenrigen/vscode-jupytext-sync",
  "repository": {
    "type": "git",
    "url": "https://github.com/caenrigen/vscode-jupytext-sync.git"
  },
  "bugs": {
    "url": "https://github.com/caenrigen/vscode-jupytext-sync/issues"
  },
  "icon": "icon.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Jupytext Paired Jupyter Notebooks",
      "properties": {
        "jupytext-paired.jupytextCommand": {
          "type": "string",
          "default": "jupytext",
          "description": "The command which runs the Jupytext CLI. Can be an absolute path or a command available in the user $PATH."
        },
        "jupytext-paired.defaultFormat": {
          "type": "string",
          "enum": [
            "hydrogen",
            "light",
            "markdown",
            "myst",
            "nomarker",
            "pandoc",
            "percent",
            "quarto",
            "rmarkdown",
            "spin",
            "sphinx"
          ],
          "default": "light",
          "description": "The default format to suggest when pairing a notebook with a script."
        }
      }
    },
    "commands": [
      {
        "command": "jupytext-sync.pairFile",
        "title": "Jupytext: Pair file with script(s)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "vsix": "vsce package -o jupytext-sync.vsix"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}